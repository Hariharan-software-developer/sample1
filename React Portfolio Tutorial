<span class="hljs-keyword">import</span> <span class="hljs-title class_">ProductList</span> <span class="hljs-keyword">from</span> <span class="hljs-string">"./components.ProductList"</span>;

<span class="hljs-keyword">const</span> <span class="hljs-title function_">App</span> = (<span class="hljs-params"></span>) =&gt; {
  <span class="hljs-keyword">const</span> [user, setUser] = <span class="hljs-title function_">useState</span>(<span class="hljs-literal">null</span>);
  <span class="hljs-keyword">const</span> [products, setProducts] = <span class="hljs-title function_">useState</span>([]);

  <span class="hljs-keyword">const</span> <span class="hljs-title function_">handleLogin</span> = (<span class="hljs-params">username</span>) =&gt; {
    <span class="hljs-title function_">setUser</span>(username);
  };

  <span class="hljs-keyword">const</span> <span class="hljs-title function_">handleAddAd</span> = (<span class="hljs-params">ad</span>) =&gt; {
    <span class="hljs-title function_">setProducts</span>([...products, ad]);
  };

  <span class="hljs-keyword">const</span> <span class="hljs-title function_">handleDeleteAd</span> = (<span class="hljs-params">index</span>) =&gt; {
    <span class="hljs-keyword">const</span> updatedProducts = products.<span class="hljs-title function_">filter</span>(<span class="hljs-function">(<span class="hljs-params">_, i</span>) =&gt;</span> i !== index);
    <span class="hljs-title function_">setProducts</span>(updatedProducts);
  };

  <span class="hljs-keyword">return</span> (
    <span class="xml"><span class="hljs-tag">&lt;<span class="hljs-name">div</span>&gt;</span>
      <span class="hljs-tag">&lt;<span class="hljs-name">Navbar</span> /&gt;</span>
      {!user ? (
        <span class="hljs-tag">&lt;<span class="hljs-name">Login</span> <span class="hljs-attr">onLogin</span>=<span class="hljs-string">{handleLogin}</span> /&gt;</span>
      ) : (
        <span class="hljs-tag">&lt;<span class="hljs-name">div</span>&gt;</span>
          <span class="hljs-tag">&lt;<span class="hljs-name">h2</span> <span class="hljs-attr">style</span>=<span class="hljs-string">{{</span> <span class="hljs-attr">textAlign:</span> "<span class="hljs-attr">center</span>" }}&gt;</span>Welcome, {user}!<span class="hljs-tag">&lt;/<span class="hljs-name">h2</span>&gt;</span>
          <span class="hljs-tag">&lt;<span class="hljs-name">CreateAd</span> <span class="hljs-attr">onAddAd</span>=<span class="hljs-string">{handleAddAd}</span> /&gt;</span>
          <span class="hljs-tag">&lt;<span class="hljs-name">ProductList</span> <span class="hljs-attr">products</span>=<span class="hljs-string">{products}</span> <span class="hljs-attr">onDeleteAd</span>=<span class="hljs-string">{handleDeleteAd}</span> /&gt;</span>
        <span class="hljs-tag">&lt;/<span class="hljs-name">div</span>&gt;</span>
      )}
    <span class="hljs-tag">&lt;/<span class="hljs-name">div</span>&gt;</span></span>
  );
};

