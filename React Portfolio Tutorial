      {leaveDetails &amp;&amp; (
        <span class="hljs-tag">&lt;<span class="hljs-name">div</span>&gt;</span>
          <span class="hljs-tag">&lt;<span class="hljs-name">h3</span>&gt;</span>Leave Details for {leaveDetails.username}<span class="hljs-tag">&lt;/<span class="hljs-name">h3</span>&gt;</span>
