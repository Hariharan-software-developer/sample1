<span class="hljs-keyword">const</span> <span class="hljs-title function_">Login</span> = (<span class="hljs-params">{ onLogin }</span>) =&gt; {
  <span class="hljs-keyword">const</span> [username, setUsername] = <span class="hljs-title function_">useState</span>(<span class="hljs-string">""</span>);

  <span class="hljs-keyword">const</span> <span class="hljs-title function_">handleSubmit</span> = (<span class="hljs-params">e</span>) =&gt; {
    e.<span class="hljs-title function_">preventDefault</span>();
    <span class="hljs-keyword">if</span> (username.<span class="hljs-title function_">trim</span>()) {
      <span class="hljs-title function_">onLogin</span>(username);
    } <span class="hljs-keyword">else</span> {
      <span class="hljs-title function_">alert</span>(<span class="hljs-string">"Please enter a username!"</span>);
    }
  };

  <span class="hljs-keyword">return</span> (
    <span class="xml"><span class="hljs-tag">&lt;<span class="hljs-name">div</span> <span class="hljs-attr">style</span>=<span class="hljs-string">{styles.container}</span>&gt;</span>
