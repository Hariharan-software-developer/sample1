      <span class="hljs-keyword">const</span> response = <span class="hljs-keyword">await</span> axios.<span class="hljs-title function_">get</span>(<span class="hljs-string">`http://localhost:5000/api/leaves/<span class="hljs-subst">${username}</span>`</span>);
