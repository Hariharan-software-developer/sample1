<span class="hljs-keyword">const</span> <span class="hljs-title function_">Navbar</span> = (<span class="hljs-params"></span>) =&gt; {
  <span class="hljs-keyword">return</span> (
    <span class="xml"><span class="hljs-tag">&lt;<span class="hljs-name">nav</span> <span class="hljs-attr">style</span>=<span class="hljs-string">{styles.nav}</span>&gt;</span>
      <span class="hljs-tag">&lt;<span class="hljs-name">h1</span> <span class="hljs-attr">style</span>=<span class="hljs-string">{styles.title}</span>&gt;</span>Classifieds App<span class="hljs-tag">&lt;/<span class="hljs-name">h1</span>&gt;</span>
    <span class="hljs-tag">&lt;/<span class="hljs-name">nav</span>&gt;</span></span>
  );
};

<span class="hljs-keyword">const</span> styles = {
  <span class="hljs-attr">nav</span>: { <span class="hljs-attr">backgroundColor</span>: <span class="hljs-string">"#3b5998"</span>, <span class="hljs-attr">padding</span>: <span class="hljs-string">"1em"</span>, <span class="hljs-attr">textAlign</span>: <span class="hljs-string">"center"</span> },
