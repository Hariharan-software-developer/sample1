  <span class="hljs-keyword">return</span> (
    <span class="xml"><span class="hljs-tag">&lt;<span class="hljs-name">li</span> <span class="hljs-attr">className</span>=<span class="hljs-string">{</span>`<span class="hljs-attr">task</span> ${<span class="hljs-attr">task.completed</span> ? "<span class="hljs-attr">completed</span>" <span class="hljs-attr">:</span> ""}`}&gt;</span>
