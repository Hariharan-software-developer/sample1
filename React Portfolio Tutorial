<span class="hljs-keyword">const</span> mongoose = <span class="hljs-built_in">require</span>(<span class="hljs-string">"mongoose"</span>);
<span class="hljs-keyword">const</span> bodyParser = <span class="hljs-built_in">require</span>(<span class="hljs-string">"body-parser"</span>);
<span class="hljs-keyword">const</span> cors = <span class="hljs-built_in">require</span>(<span class="hljs-string">"cors"</span>);

<span class="hljs-keyword">const</span> leaveRoutes = <span class="hljs-built_in">require</span>(<span class="hljs-string">"./routes/leaveRoutes"</span>);

<span class="hljs-keyword">const</span> app = <span class="hljs-title function_">express</span>();
app.<span class="hljs-title function_">use</span>(<span class="hljs-title function_">cors</span>());
app.<span class="hljs-title function_">use</span>(bodyParser.<span class="hljs-title function_">json</span>());

<span class="hljs-comment">// MongoDB Connection</span>
mongoose
  .<span class="hljs-title function_">connect</span>(<span class="hljs-string">"mongodb://127.0.0.1:27017/leaveManagement"</span>, { <span class="hljs-attr">useNewUrlParser</span>: <span class="hljs-literal">true</span>, <span class="hljs-attr">useUnifiedTopology</span>: <span class="hljs-literal">true</span> })
  .<span class="hljs-title function_">then</span>(<span class="hljs-function">() =&gt;</span> <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">log</span>(<span class="hljs-string">"MongoDB connected"</span>))
  .<span class="hljs-title function_">catch</span>(<span class="hljs-function">(<span class="hljs-params">err</span>) =&gt;</span> <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"MongoDB connection error:"</span>, err));

<span class="hljs-comment">// Routes</span>
app.<span class="hljs-title function_">use</span>(<span class="hljs-string">"/api/leaves"</span>, leaveRoutes);

<span class="hljs-comment">// Start the server</span>
<span class="hljs-keyword">const</span> <span class="hljs-variable constant_">PORT</span> = <span class="hljs-number">5000</span>;
app.<span class="hljs-title function_">listen</span>(<span class="hljs-variable constant_">PORT</span>, <span class="hljs-function">() =&gt;</span> <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">log</span>(<span class="hljs-string">`Server running on http://localhost:<span class="hljs-subst">${PORT}</span>`</span>));
</code></div></div></pre><hr><h5><strong>Leave Model (<code>models/Leave.js</code>)</strong></h5><pre class="!overflow-visible"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary dark:bg-gray-950"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-md h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none">javascript</div><div class="sticky top-9 md:top-[5.75rem]"><div class="absolute bottom-0 right-2 flex h-9 items-center"><div class="flex items-center rounded bg-token-sidebar-surface-primary px-2 font-sans text-xs text-token-text-secondary dark:bg-token-main-surface-secondary"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none py-1"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copy code</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre hljs language-javascript"><span class="hljs-keyword">const</span> mongoose = <span class="hljs-built_in">require</span>(<span class="hljs-string">"mongoose"</span>);

<span class="hljs-keyword">const</span> <span class="hljs-title class_">LeaveSchema</span> = <span class="hljs-keyword">new</span> mongoose.<span class="hljs-title class_">Schema</span>({
  <span class="hljs-attr">username</span>: { <span class="hljs-attr">type</span>: <span class="hljs-title class_">String</span>, <span class="hljs-attr">required</span>: <span class="hljs-literal">true</span> },
  <span class="hljs-attr">leaveType</span>: { <span class="hljs-attr">type</span>: <span class="hljs-title class_">String</span>, <span class="hljs-attr">required</span>: <span class="hljs-literal">true</span> },
  <span class="hljs-attr">daysRequested</span>: { <span class="hljs-attr">type</span>: <span class="hljs-title class_">Number</span>, <span class="hljs-attr">required</span>: <span class="hljs-literal">true</span> },
  <span class="hljs-attr">remainingLeaves</span>: {
    <span class="hljs-attr">casualLeave</span>: { <span class="hljs-attr">type</span>: <span class="hljs-title class_">Number</span>, <span class="hljs-attr">default</span>: <span class="hljs-number">12</span> },
    <span class="hljs-attr">medicalLeave</span>: { <span class="hljs-attr">type</span>: <span class="hljs-title class_">Number</span>, <span class="hljs-attr">default</span>: <span class="hljs-number">8</span> },
  },
});

<span class="hljs-variable language_">module</span>.<span class="hljs-property">exports</span> = mongoose.<span class="hljs-title function_">model</span>(<span class="hljs-string">"Leave"</span>, <span class="hljs-title class_">LeaveSchema</span>);
</code></div></div></pre><hr><h5><strong>Leave Routes (<code>routes/leaveRoutes.js</code>)</strong></h5><pre class="!overflow-visible"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary dark:bg-gray-950"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-md h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none">javascript</div><div class="sticky top-9 md:top-[5.75rem]"><div class="absolute bottom-0 right-2 flex h-9 items-center"><div class="flex items-center rounded bg-token-sidebar-surface-primary px-2 font-sans text-xs text-token-text-secondary dark:bg-token-main-surface-secondary"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none py-1"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copy code</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre hljs language-javascript"><span class="hljs-keyword">const</span> express = <span class="hljs-built_in">require</span>(<span class="hljs-string">"express"</span>);
<span class="hljs-keyword">const</span> <span class="hljs-title class_">Leave</span> = <span class="hljs-built_in">require</span>(<span class="hljs-string">"../models/Leave"</span>);
<span class="hljs-keyword">const</span> router = express.<span class="hljs-title class_">Router</span>();

<span class="hljs-comment">// Apply for Leave</span>
router.<span class="hljs-title function_">post</span>(<span class="hljs-string">"/apply"</span>, <span class="hljs-keyword">async</span> (req, res) =&gt; {
  <span class="hljs-keyword">const</span> { username, leaveType, daysRequested } = req.<span class="hljs-property">body</span>;

  <span class="hljs-keyword">try</span> {
    <span class="hljs-keyword">const</span> leave = <span class="hljs-keyword">await</span> <span class="hljs-title class_">Leave</span>.<span class="hljs-title function_">findOne</span>({ username });

    <span class="hljs-keyword">if</span> (!leave) {
      <span class="hljs-comment">// Create a new record if user not found</span>
      <span class="hljs-keyword">const</span> newLeave = <span class="hljs-keyword">new</span> <span class="hljs-title class_">Leave</span>({ username, leaveType, daysRequested });
      <span class="hljs-keyword">if</span> (leaveType === <span class="hljs-string">"casualLeave"</span>) {
        newLeave.<span class="hljs-property">remainingLeaves</span>.<span class="hljs-property">casualLeave</span> -= daysRequested;
      } <span class="hljs-keyword">else</span> <span class="hljs-keyword">if</span> (leaveType === <span class="hljs-string">"medicalLeave"</span>) {
        newLeave.<span class="hljs-property">remainingLeaves</span>.<span class="hljs-property">medicalLeave</span> -= daysRequested;
      }
      <span class="hljs-keyword">await</span> newLeave.<span class="hljs-title function_">save</span>();
      <span class="hljs-keyword">return</span> res.<span class="hljs-title function_">status</span>(<span class="hljs-number">201</span>).<span class="hljs-title function_">json</span>(newLeave);
    }

    <span class="hljs-comment">// Update existing record</span>
    <span class="hljs-keyword">if</span> (leaveType === <span class="hljs-string">"casualLeave"</span>) {
      leave.<span class="hljs-property">remainingLeaves</span>.<span class="hljs-property">casualLeave</span> -= daysRequested;
    } <span class="hljs-keyword">else</span> <span class="hljs-keyword">if</span> (leaveType === <span class="hljs-string">"medicalLeave"</span>) {
      leave.<span class="hljs-property">remainingLeaves</span>.<span class="hljs-property">medicalLeave</span> -= daysRequested;
    }

    <span class="hljs-keyword">await</span> leave.<span class="hljs-title function_">save</span>();
    res.<span class="hljs-title function_">status</span>(<span class="hljs-number">200</span>).<span class="hljs-title function_">json</span>(leave);
  } <span class="hljs-keyword">catch</span> (err) {
    res.<span class="hljs-title function_">status</span>(<span class="hljs-number">500</span>).<span class="hljs-title function_">json</span>({ <span class="hljs-attr">error</span>: <span class="hljs-string">"Error applying for leave"</span> });
  }
});

<span class="hljs-comment">// Get Leave Details</span>
router.<span class="hljs-title function_">get</span>(<span class="hljs-string">"/:username"</span>, <span class="hljs-keyword">async</span> (req, res) =&gt; {
  <span class="hljs-keyword">const</span> { username } = req.<span class="hljs-property">params</span>;

  <span class="hljs-keyword">try</span> {
    <span class="hljs-keyword">const</span> leave = <span class="hljs-keyword">await</span> <span class="hljs-title class_">Leave</span>.<span class="hljs-title function_">findOne</span>({ username });
    <span class="hljs-keyword">if</span> (!leave) <span class="hljs-keyword">return</span> res.<span class="hljs-title function_">status</span>(<span class="hljs-number">404</span>).<span class="hljs-title function_">json</span>({ <span class="hljs-attr">error</span>: <span class="hljs-string">"User not found"</span> });
    res.<span class="hljs-title function_">status</span>(<span class="hljs-number">200</span>).<span class="hljs-title function_">json</span>(leave);
  } <span class="hljs-keyword">catch</span> (err) {
    res.<span class="hljs-title function_">status</span>(<span class="hljs-number">500</span>).<span class="hljs-title function_">json</span>({ <span class="hljs-attr">error</span>: <span class="hljs-string">"Error fetching leave details"</span> });
  }
});

<span class="hljs-variable language_">module</span>.<span class="hljs-property">exports</span> = router;
