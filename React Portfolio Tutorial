      {posts.length &gt; 0 ? (
        posts.map((post, index) =&gt; (
          <span class="hljs-tag">&lt;<span class="hljs-name">div</span> <span class="hljs-attr">key</span>=<span class="hljs-string">{index}</span> <span class="hljs-attr">style</span>=<span class="hljs-string">{styles.post}</span>&gt;</span>
            <span class="hljs-tag">&lt;<span class="hljs-name">p</span>&gt;</span>{post}<span class="hljs-tag">&lt;/<span class="hljs-name">p</span>&gt;</span>
